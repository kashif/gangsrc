CMCLab 1.0

name Bubbleton in S^3 on cylinder
potential {{
  0,
  1/3/t + 1/3 },{
  1/3 + 1/3 t,
  0
}}

domain rectangular -3.25; 5; 100; -3.25+2pi i; 100;
potential_variable z
zstart 1
lambda S3 atan[3Sqrt[7]]/(2Pi); -atan[3Sqrt[7]]/(2Pi)
normalshift 1
mean_curvature 1
polynomial_length 10
basis_length 20
iwasawa_method qr
birkhoff true
one_column false
initial_integrator_count 100
integrator_count 10
xstart {{t-(7+3Sqrt[5])/2, 0},{0, (7+3Sqrt[5])/2 t-1}}
