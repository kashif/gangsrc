KLab 1.0

name Pseudosphere

potential_x {{i/(2 Sqrt[2]), 0.25/t + 0.25i t}, {-0.25/t + 0.25i t, -i/(2 Sqrt[2])}}

potential_y {{i/(2 Sqrt[2]), 0.25/t + 0.25i t}, {-0.25/t + 0.25i t, -i/(2 Sqrt[2])}}

domain rectangular -pi-12i; pi-12i; 15; -pi+10i; 64;

zstart 0
lambda 1
polynomial_length 20

initial_x {{1,0},{0,1}}

initial_y {{1,0},{0,1}}

initial_integrator_count 100
integrator_count 1
